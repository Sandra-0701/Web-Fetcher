{
    "version": 2,
    "builds": [
      {
        "src": "front/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"  // Adjust this if your build directory is different
        }
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "front/$1"
      }
    ]
  }
  